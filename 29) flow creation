public class Main
{
    public static void main(String[] args) {
        Thread t = new Thread();
        t.start();
        
        System.out.println("hello");
        
        
    }
}

    class Red implements Runnable{
      @Override 
       public void run(){
         for(int i = 0; i < 100;i++){
         System.out.println("Run" + i);
         }
       }
    }
    
