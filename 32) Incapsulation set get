public class Program
{
    public static void main(String[] args) {
    Human alisa = new Human(); 
    alisa.setName("Alisa");
    alisa.setAge(22);
System.out.println("Out in main "+ alisa.getName());    
System.out.println("Out in main "+alisa.getAge());
    
        
    }
}

class Human{
    private String name;
    private int age;
public void setName(String userName){
    name = userName; 
}
public String getName(){
    return name;
}
public void setAge(int userAge){
    age = userAge;
}
public int getAge(){
    return age;
}

}
